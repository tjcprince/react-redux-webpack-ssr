{
  "name": "react-redux-webpack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare":
      "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build":
      "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static/server",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 20000",
    "prod:start": "npm-run-all prod:client:build prod:server:build prod:server:start",
    "prod:client:build":
      "webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build":
      "webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "better-npm-run prod:server:start"
  },
  "betterScripts": {
    "prod:server:start": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "node ./src/server/start.babel.js"
    }
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "setupFiles": ["<rootDir>/__mocks__/test-setup.js"]
  },
